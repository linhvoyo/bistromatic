NAME = eval_expr
SRCS = eval_expr.c ft_util.c

all: build

build:
	gcc -Wall -Werror -Wextra $(SRCS) -o $(NAME)

clean:
	rm -f $(NAME)

fclean: clean
	rm -f $(NAME)
